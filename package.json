{
  "name": "shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build:server": "tsc",
    "build:dashboard": "vite build",
    "build": "yarn run build:server && yarn run build:dashboard",
    "build:seed": "tsc && cp -R ./static ./dist/static",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "seed:prod": "node ./dist/populate/seed.js",
    "seed:dev": "ts-node ./src/populate/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.908.0",
    "@aws-sdk/lib-storage": "3.908.0",
    "@pinelab/vendure-plugin-coinbase": "^1.4.2",
    "@clerk/backend": "^2.29.3",
    "@clerk/clerk-react": "^5.59.5",
    "@vendure/admin-ui-plugin": "3.5.0",
    "@vendure/asset-server-plugin": "3.5.0",
    "@vendure/core": "3.5.0",
    "@vendure/create": "3.5.0",
    "@vendure/dashboard": "3.5.1",
    "@vendure/email-plugin": "3.5.0",
    "@vendure/graphiql-plugin": "3.5.0",
    "axios": "^1.12.2",
    "crypto": "^1.0.1",
    "dotenv": "17.2.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mercadopago": "^2.10.0",
    "pg": "8.16.3",
    "resend": "^6.0.2"
  },
  "devDependencies": {
    "@types/react": "19.2.9",
    "@vendure/cli": "3.3.7",
    "@vendure/ui-devkit": "3.5.0",
    "concurrently": "9.2.0",
    "typescript": "5.8.3"
  },
  "overrides": {
    "@swc/core": "1.13.5",
    "@lingui/swc-plugin": "5.6.1"
  }
}
